{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "flowcore-analytics-collector",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/",
    "flowcore:apply:data-core": "flowcore data-core apply -f flowcore.yaml -f flowcore.local.yaml",
    "flowcore:apply:proxy": "flowcore scenario apply -f flowcore.yaml",
    "flowcore:local:proxy": "env-cmd -f .env -- sh -c 'flowcore scenario local -f flowcore.yaml -f flowcore.local.yaml -f flowcore.local.development.yaml -s now -e http://localhost:3005/api/transformer -H \"X-Secret: $FLOWCORE_TRANSFORMER_SECRET\"'"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/swagger": "^1.3.0",
    "@flowcore/data-pump": "^0.15.0",
    "@flowcore/pathways": "^0.16.1",
    "@flowcore/sdk": "^1.49.0",
    "@sinclair/typebox": "^0.34.33",
    "crypto": "^1.0.1",
    "elysia": "latest",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "bun-types": "latest",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "env-cmd": "^10.1.0"
  },
  "module": "src/index.js"
}